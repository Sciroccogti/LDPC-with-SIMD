{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cmake.configureOnOpen": true,
    "files.associations": {
        "*.mcfunction": "mcfunction",
        "iostream": "cpp",
        "fstream": "cpp"
    },
    "cmake.configureArgs": [
        "-DEIGEN_ENABLE_AVX512=ON",
        "-Dmatch=native",
    ]
}